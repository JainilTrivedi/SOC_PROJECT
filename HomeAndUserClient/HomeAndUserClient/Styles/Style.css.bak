.table_container {
    height: 20vh;
    width: 40vw;
    font-family: 'Poppins';
}

.outer_container {
    display: flex;
    align-items: center;
    justify-content: center;
}

* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'poppins',sans-serif;
}

.home_container {
    width: 100%;
    height: 100vh;
    background-image: linear-gradient(#fefbd8,transparent);
    background-size: cover;
    background-position: center;
    position: relative;
	    border-collapse: collapse;
}

.table_text {
    font-size: 1.2rem;
    color: black;
}

.table_icon {
    font-size: 1.5rem;
    color: black;
    transition: 0.3s;
}


.nav_logo {
    width: 120px;
    height:70px;
    cursor: pointer;
}

nav {
    z-index: 1;
    position: fixed;
    display: flex;
    width: 100%;
    align-items: center;
    flex-wrap: wrap;
    background-color: #80ced6;
}

    nav ul {
        flex: 1;
        text-align: right;
        padding-right: 30px;
    }

        nav ul li {
            display: inline-block;
            list-style: none;
            margin: 10px 30px;
        }
.nav_text:hover {
    text-decoration: none;
    color: #4040a1;
}

@keyframes fadeIn {
    from {
        opacity: 0;
    }
}

.nav_text {
    font-size: 1.3rem;
    border: none;
    cursor: pointer;
    background: none;
    color: #fefbd8
}

h1 {
    padding-top:1%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.buttons {
    font-size: 1.3rem;
    cursor: pointer;
    display: flex;
    align-items: flex-end;
}

    .buttons:hover {
        background-color: #80ced6;
    }

h2 {
    padding-left: 2px;
    font-family: 'Poppins';
    color: #fefbd8
}





tr {
    border-top: 1px solid #ccc;
}

th {
    text-align: left;
}
